<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="defaultStatementTimeout" value="25"/>
        <setting name="defaultFetchSize" value="100"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias alias="UserEntity" type="com.scd.dcs.domains.entities.UserEntity"/>
        <typeAlias alias="EmailAuthEntity" type="com.scd.dcs.domains.entities.EmailAuthEntity"/>
        <typeAlias alias="ArticleEntity" type="com.scd.dcs.domains.entities.ArticleEntity"/>
        <typeAlias alias="BoardEntity" type="com.scd.dcs.domains.entities.BoardEntity"/>
        <typeAlias alias="CommentEntity" type="com.scd.dcs.domains.entities.CommentEntity"/>
        <typeAlias alias="BoardVo" type="com.scd.dcs.domains.vos.BoardVo"/>
        <typeAlias alias="UserProperty" type="com.scd.dcs.domains.vos.UserProperty"/>

    </typeAliases>
</configuration>